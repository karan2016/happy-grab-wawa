{
  "version": "1.2.2",
  "author": "Anton Fisher <a.fschr@gmail.com> (http://antonfisher.com)",
  "description": "React boilerplate with ES2015, Express.js, and Webpack 3",
  "license": "MIT",
  "engines": {
    "node": "8.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antonfisher/react-express-webpack.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./src/server",
    "start": "NODE_ENV=production node ./build/server",
    "prebuild": "mkdir -p ./build; rm -rf ./build/*",
    "build": "npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack -p --display-error-details --config=config/webpack.config.prod.js",
    "build:server": "cp -r src/server build/server",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern node_modules -- .",
    "test": "exit 1",
    "release:patch": "npm run lint && npx standard-version -r patch && git push --follow-tags origin master",
    "release:minor": "npm run lint && npx standard-version -r minor && git push --follow-tags origin master",
    "release:major": "npm run lint && npx standard-version -r major && git push --follow-tags origin master"
  },
  "postcss": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-watch": "2.0.7",
    "css-loader": "0.28.7",
    "eslint": "4.7.2",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-airbnb-base": "12.0.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "html-loader": "0.5.1",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-sass": "4.5.3",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.0.6",
    "rc-banner-anim": "^1.0.0",
    "rc-scroll-anim": "^2.0.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2"
  },
  "dependencies": {
    "antd": "^2.13.8",
    "axios": "^0.17.0",
    "babel-polyfill": "6.26.0",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "enquire": "^0.0.2",
    "express": "4.15.5",
    "flexiblejs": "0.0.1",
    "history": "4.7.2",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "jquery": "^3.2.1",
    "material-ui": "0.19.2",
    "nodemon": "^1.12.1",
    "prop-types": "15.6.0",
    "rc-queue-anim": "^1.3.1",
    "rc-tween-one": "^1.5.5",
    "react": "16.0.0",
    "react-document-meta": "^2.1.2",
    "react-dom": "16.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-intl": "2.4.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
